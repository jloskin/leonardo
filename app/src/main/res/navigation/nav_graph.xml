<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_graph"
    app:startDestination="@id/categoriesFragment">
    <fragment
        android:id="@+id/categoriesFragment"
        android:name="petproject.loskin.leonardo.presentation.ui.magazine.categories.CategoriesFragment"
        android:label="CategoriesFragment">
        <action
            android:id="@+id/action_magazineFragment_to_magazineCategoryFragment2"
            app:destination="@id/subCategoriesFragment" />
        <action
            android:id="@+id/action_categoriesFragment_to_authorizeFragment"
            app:destination="@id/authorizeFragment" />
    </fragment>
    <fragment
        android:id="@+id/subCategoriesFragment"
        android:name="petproject.loskin.leonardo.presentation.ui.magazine.subcategories.SubCategoriesFragment"
        android:label="SubCategoriesFragment">
        <action
            android:id="@+id/action_magazineCategoryFragment2_to_goodsFragment"
            app:destination="@id/goodsFragment" />
        <action
            android:id="@+id/action_subCategoriesFragment_to_authorizeFragment"
            app:destination="@id/authorizeFragment" />
    </fragment>
    <fragment
        android:id="@+id/goodsFragment"
        android:name="petproject.loskin.leonardo.presentation.ui.magazine.goods.GoodsFragment"
        android:label="GoodsFragment" >
        <action
            android:id="@+id/action_goodsFragment_to_authorizeFragment"
            app:destination="@id/authorizeFragment" />
    </fragment>
    <fragment
        android:id="@+id/authorizeFragment"
        android:name="petproject.loskin.leonardo.presentation.ui.authorize.AuthorizeFragment"
        android:label="AuthorizeFragment" >
        <action
            android:id="@+id/action_authorizeFragment_to_categoriesFragment"
            app:destination="@id/categoriesFragment" />
    </fragment>
</navigation>